  "scripts": {
    "deploy2": "yarn build && bash deploy2.sh",
    "start:pm2": "pm2 start yarn --name next-9000-prod -- start",
    "envs-init": "bash bin/envs-init.sh nextjs",
    "local:cleanup": "bash bin/local-cleanup.sh nextjs",
    "deploy:prod:build-send-restart-all": "deploy-app -d prod:build-send-restart-all",
    "deploy:prod:build-send-restart-front": "deploy-app -d prod:build-send-restart-front",
    "deploy:prod:send-restart-front": "deploy-app -d prod:send-restart-front",
    "dev": "yarn build && NODE_ENV=development node server.dist/run.js",
    "build": "yarn transpile-server && next build",
    "start": "NODE_ENV=production node server.dist/run.js",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "transpile-server": "node_modules/.bin/tsc --downlevelIteration --project tsconfig.server.json",
    "test": "jest",
    "_partytown": "node_modules/@builder.io/partytown/bin/partytown.cjs copylib ./public/static/~partytown"
  },
  "_moduleAliases": {
    "$tests": "./tests",
    "~": "./"
  },
  "jest": {
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>$1",
      "^@src(.*)$": "<rootDir>/src$1",
      "^@srv.utils(.*)$": "<rootDir>/srv.utils$1"
    }
