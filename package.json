{
  "name": "frontend.nextjs",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "deploy2": "yarn build && bash deploy2.sh",
    "start:pm2": "pm2 start yarn --name next-9000-prod -- start",
    "envs-init": "bash bin/envs-init.sh nextjs",
    "local:cleanup": "bash bin/local-cleanup.sh nextjs",
    "deploy:prod:build-send-restart-all": "deploy-app -d prod:build-send-restart-all",
    "deploy:prod:build-send-restart-front": "deploy-app -d prod:build-send-restart-front",
    "deploy:prod:send-restart-front": "deploy-app -d prod:send-restart-front",
    "dev": "yarn build && NODE_ENV=development node server.dist/run.js",
    "build": "yarn transpile-server && next build",
    "start": "NODE_ENV=production node server.dist/run.js",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "transpile-server": "node_modules/.bin/tsc --downlevelIteration --project tsconfig.server.json"
  },
  "_moduleAliases": {
    "$tests": "./tests",
    "~": "./"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.95",
    "@mui/material": "latest",
    "@mui/styles": "^5.0.1",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.21.4",
    "axios-retry": "^3.3.1",
    "better-module-alias": "^1.2.1",
    "body-parser": "^1.20.2",
    "clsx": "^1.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "fast-deep-equal": "^3.1.3",
    "js-cookie": "^3.0.1",
    "next": "latest",
    "next-redux-wrapper": "^7.0.5",
    "notistack": "^3.0.1",
    "react": "17.x",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "17.x",
    "react-hook-form": "^7.45.4",
    "react-intl-universal": "^2.4.8",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-symbiote": "^3.4.0",
    "slugify": "^1.6.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^2.2.7",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "latest",
    "@types/redux-persist": "^4.3.1",
    "deploy-app": "^1.0.3",
    "typescript": "4.x"
  }
}
